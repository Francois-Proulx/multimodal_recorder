<launch>
  <arg name="use_sim_time" default="false" description="Use simulation clock if true"/>
  <node pkg="video" name="video_quat" exec="video_quat" output="screen">
    <param from="$(find-pkg-share video)/config/video_quat_config.yaml" />
    <param name="use_sim_time" value="$(var use_sim_time)" />
  </node>
</launch>
