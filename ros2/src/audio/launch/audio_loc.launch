<launch>
  <arg name="use_sim_time" default="false" description="Use simulation clock if true"/>
  <node pkg="audio" name="audio_loc" exec="audio_loc" output="screen">
    <param from="$(find-pkg-share audio)/config/audio_params.yaml" />
    <param name="use_sim_time" value="$(var use_sim_time)" />
  </node>
</launch>
